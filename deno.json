{
  "imports": {
    "lume/": "https://deno.land/x/lume@v2.5.0/",
    "esbuild/": "https://deno.land/x/esbuild@v0.19.9/",
    "sha256/": "https://denopkg.com/chiefbiiko/sha256@v1.0.0/",
    "lightningcss-wasm": "npm:lightningcss-wasm@1.22.1/",
    "$/": "./"
  },
  "lock": false,
  "tasks": {
    "build": "deno task lume",
    "serve": "deno task lume -s",
    "lume": "echo \"import 'lume/cli.ts'\" | deno run -A -"
  },
  "compilerOptions": {
    "types": ["lume/types.ts"]
  }
}
